; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Explosion",
"language": "GDScript",
"path": "res://Explosion.gd"
}, {
"base": "Node",
"class": "weapon_setter",
"language": "GDScript",
"path": "res://weapons/WeaponPreseter.gd"
} ]
_global_script_class_icons={
"Explosion": "",
"weapon_setter": ""
}

[application]

config/name="Slayer"
run/main_scene="res://MainMenu.tscn"
config/icon="res://assets/sprites/icon.png"

[autoload]

FullscreenCRT="*res://misc/FullscreenCRT.tscn"
FPSCounter="*res://misc/FPSCounter.tscn"
Global="*res://Global.gd"
Console="*res://addons/quentincaffeino-console/src/Console.tscn"
CustomCommands="*res://misc/CustomCommands.gd"
Server="*res://singletons/server.gd"

[debug]

gdscript/completion/autocomplete_setters_and_getters=true
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "gut", "quentincaffeino-console" )

[gui]

theme/custom="res://assets/ui/SlayerTheme.tres"
theme/custom_font="res://assets/fonts/savior.tres"

